[Desktop Entry]
# Override desktop file in /usr/share/applications to hide from menu
Hidden=true

Version=1.0
Type=Application
Exec=exo-open --launch MailReader %u
Icon=org.xfce.mailreader
StartupNotify=true
Terminal=false
Categories=Network;X-XFCE;X-Xfce-Toplevel;
OnlyShowIn=XFCE;
X-XFCE-MimeType=x-scheme-handler/mailto;
X-AppStream-Ignore=True
Name=Mail Reader
Name[tr]=E-posta Okuyucusu
Comment=Read your email
Comment[tr]=E-postanızı okuyun
